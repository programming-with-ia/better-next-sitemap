{
  "name": "better-next-sitemap",
  "version": "0.0.1",
  "description": "A better sitemap generator for Next.js with support for large sitemaps, images, and videos.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "license": "MIT",
  "author": "programming-with-ia",
  "repository": {
    "type": "git",
    "url": "https://github.com/programming-with-ia/better-next-sitemap"
  },
  "keywords": [
    "nextjs",
    "sitemap",
    "generator",
    "seo",
    "dynamic-sitemap"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup src/index.ts --format esm --watch --dts --clean --minify false",
    "build": "tsup src/index.ts --format esm --dts --clean --minify"
  },
  "devDependencies": {
    "next": "16.1.6",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "next": "^13.0.0 || ^14.0.0 || ^15.0.0 || ^16.0.0"
  },
  "publishConfig": {
    "access": "public",
    "tag": "alpha"
  }
}
